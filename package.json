  "description": "If you want to get s****d + f****d at the log cabin, then complete this quiz.",
  "main": "index.js",
  "scripts": {
    "test": "BABEL_ENV=unittest ALLOW_CONFIG_MUTATIONS=true mocha --opts ./mocha.opts",
    "test:coverage": "nyc --reporter=html yarn test",
    "start": "node dist/app/server.js",
    "build": "webpack --mode production",
    "build:dev": "webpack --mode development",
    "pretty": "prettier --write '{config,src,test}/**/*.{js,jsx,scss,json}' '*.{js,json,yml}'",
    "lint": "eslint --ext .js,.jsx src test",
    "watch": "webpack --mode development --config webpack.config.js --watch",
    "clinic:doctor": "clinic doctor -- node dist/app/server.js",
    "clinic:flame": "clinic flame -- node dist/app/server.js",
    "autocannon": "autocannon -c100 localhost:8080"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/craigharvi3/coderoyale.git"
  },
